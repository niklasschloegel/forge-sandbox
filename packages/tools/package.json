{
  "name": "tools",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "../../node_modules/.bin/npm-run-all 'lint:*'",
    "lint:root-prettier": "../../node_modules/.bin/prettier \"../{**/*,*}.{css,scss,html,js,cjs,ts,jsx,tsx,md,mdx,sh,json,yaml,yml,xml}\" --check --ignore-path ../../.prettierignore --ignore-path ../../.gitignore",
    "root-prettier-write": "../../node_modules/.bin/prettier \"../{**/*,*}.{css,scss,html,js,cjs,ts,jsx,tsx,md,mdx,sh,json,yaml,yml,xml}\" --write --ignore-path ../../.prettierignore --ignore-path ../../.gitignore"
  },
  "devDependencies": {
    "linting": "0.0.0"
  }
}
